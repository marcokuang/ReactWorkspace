{
  "name": "react-ssr",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start-server": "nodemon --watch build --exec \"node build/bundle.js\" ",
    "start-build-server": "webpack --watch --config webpack.server.js",
    "start-build-client": "webpack --watch --config webpack.client.js",
    "dev": "npm-run-all --parallel start-*",
    "start": "concurrently \"yarn run start-server\" \"yarn run start-build-server\" \"yarn run start-build-client\""
  },
  "dependencies": {
    "axios": "^0.21.1",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-preset-env": "1.6.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2017": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "compression": "^1.7.4",
    "concurrently": "3.5.0",
    "express": "^4.17.3",
    "express-http-proxy": "1.0.6",
    "lodash": "^4.17.21",
    "nodemon": "^2.0.7",
    "npm-run-all": "4.1.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "5.2.0",
    "react-redux": "5.0.6",
    "react-router-config": "1.0.0-beta.4",
    "react-router-dom": "4.2.2",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0",
    "serialize-javascript": "^5.0.1",
    "webpack": "^5.24.2",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "4.1.0",
    "webpack-node-externals": "1.6.0"
  }
}
